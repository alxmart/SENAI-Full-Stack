Algoritmo "semnome"
//

Var

 num1,num2: inteiro

Inicio


   escreval("Digite o primeiro número")
   leia(num1)
   escreval("Digite o segundo número")
   leia(num2)

   se (num1 > num2) entao
      escreval("O primeiro número é maior: ", num1)
   senao
      escreval(" O segundo número é maior: ", num2)
   fimse



Fimalgoritmo